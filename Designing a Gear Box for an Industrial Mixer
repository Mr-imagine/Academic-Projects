    F2=F1a-((2*T)/d);
    Fi=(F1a+F2)/2-Fc;
    Ht=((F1a-F2)*V)/33000;
    fprim=(log((F1a-Fc)/(F2-Fc)))*(1/phi);
    nn(1,1)=double(fprim);
    nn(1,2)=double(f);
    nn(1,3)=double(Ht);
    NN={'fprim=',nn(1,1);'f=',nn(1,2);'the new H nominal _ hp=',nn(1,3)};
