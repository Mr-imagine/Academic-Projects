    if no==1
        tetad=pi-2*asin((D-d)/(2*C));
        tetaD=pi+2*asin((D-d)/(2*C));
        L=(4*C^2-(D-d)^2)^0.5+0.5*(D*tetaD+d*tetad);
        phi=tetad;
    else
        teta=pi+2*asin((D-d)/(2*C));
        L=(4*C^2-(D-d)^2)^0.5+0.5*((D+d)*teta);
        phi=teta;
    end
