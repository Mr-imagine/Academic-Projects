    Cp=input('find pulley correction factor Cp from Table 17-4   =  ');
    F1a=b*Fa*Cp*Cv;
    F2=F1a-((2*T)/d);
    Fi=(F1a+F2)/2-Fc;
    fprim=(log((F1a-Fc)/(F2-Fc)))*(1/phi);
    nn(1,1)=fprim;
    nn(1,2)=f;
    NN={'fprim',nn(1,1);'f',nn(1,2)};
