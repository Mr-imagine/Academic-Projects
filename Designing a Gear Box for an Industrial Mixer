    C(70,2)=405; 
    if E==1
        V=1;
    else
        V=1.2;
    end
    for I=1:1:5000
        x=Fa/C0;
    if x<0.014
       e=0.019;
    else
    e=- 0.9796599351*x^2 + 0.9676804793*x + 0.19748856;
    end
    if Fa/(V*Fr)<=e
        X1=1;
        Y1=0;
    else
        X1=0.56;
        Y1=7.472201716*x^2 - 6.05989608*x + 2.140081506;
    end 
    Fe=X1*V*Fr+Y1*Fa;
    C10=af*Fe*(Xd/(0.02+(4.439*(1-R)^(1/1.483))))^(1/3);
    for J=1:1:60
        if C10<=C(J,1)
            C0=C(J,2);
            break
        end
    end
    end
end
    NN={'C10 KN',C10};
    disp(NN);
